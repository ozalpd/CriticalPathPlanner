@using CriticalPath.Web.Models
@using CriticalPath.Data
@using CP.i8n
@model DiscontinueCompanyVM
@{
    ViewBag.Title = EntityStrings.Discontinued;
}
<h2 class="col-lg-offset-2 col-md-offset-3 col-sm-offset-3">@ViewBag.Title</h2>
@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="form-horizontal">
        @Html.ValidationSummary(true)
        @Html.HiddenFor(model => model.Id)

        <div class="form-group">
            @Html.LabelFor(model => model.CompanyName, htmlAttributes: new { @class = "control-label col-lg-2 col-md-3 col-sm-3" })
            <div class="col-lg-5 col-md-6 col-sm-7">
                @Html.TextBoxFor(m => m.CompanyName, new { @class = "form-control", autocomplete = "off", @readonly = "readonly" })
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(model => model.Country, htmlAttributes: new { @class = "control-label col-lg-2 col-md-3 col-sm-3" })
            <div class="col-lg-5 col-md-6 col-sm-7">
                @Html.TextBoxFor(m => m.Country, new { @class = "form-control", autocomplete = "off", @readonly = "readonly" })
            </div>
        </div>

        @Html.Partial("_Discontinued")
        @Html.Partial("_SaveButtonsPartial")
    </div>
}