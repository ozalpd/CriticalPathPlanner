@using CriticalPath.Web.Models
@using CriticalPath.Data
@using CP.i8n
@model Product
@{
    Layout = null;
}
@if (Model != null)
{
    if (Model.Category != null)
    {
            <a class="pull-left" title="@EntityStrings.Category"
            href='/ProductCategoriess/Details/@Model.CategoryId'>
            @Model.Category.Title /&nbsp;
            </a>
    }
    <a href='/Products/Details/@Model.Id'>
        @Model.Title </a >
}
